acc = {'GH':{'l_n':"A","f_n":"B","city":"TPE"},'KN':{'l_n':"C","f_n":"D","city":"BEI"}}

for a,b in acc.items():
  print(a)
  print(b['l_n']+b['f_n']+ "  " +b['city']  )

